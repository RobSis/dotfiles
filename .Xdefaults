*background:                    #262626
*foreground:                    #c2c2b0

#include "/home/rob/.xthemes/egalike"

! Urxvt specific
urxvt*perl-ext-common: default,tabbed,-selection
urxvt.urlLauncher:   /usr/bin/google-chrome
urxvt.matcher.button:  1

!  - use a bitmap font
urxvt*font:        terminus-14
!  - cursor
urxvt*cursorColor: #DCDCCC
urxvt*saveLines:    65535
urxvt*scrollBar:    false
urxvt*cursorBlink:   false
urxvt*secondaryScroll: true
urxvt*geometry: 80x25
urxvt*tabbed.tabbar-fg: 3
urxvt*tabbed.tabbar-bg: 0
urxvt*tabbed.tab-fg:    1
urxvtmtabbed.tab-bg:    0
